{
  "name": "svg",
  "version": "0.1.0",
  "description": "UI Components: SVG",
  "main": "dist/Svg.min.js",
  "esnext": "esnext/Svg.jsx",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:peter-mouland/svg.git"
  },
  "keywords": [
    "react",
    "svg"
  ],
  "author": "Peter Mouland",
  "license": "ISC",
  "scripts": {
    "--------- DEVELOPMENT -------------": "",
    "start": "start-storybook -s ./tests/fixtures -p 9001",
    "test:server": "node tests/regression/start-test-server.js",
    "build": "build-storybook -o public",
    "--------- LINTING ------------------": "",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint './esnext/**/*.jsx' './esnext/**/*.js'",
    "lint:css": "stylelint 'packages/*/src/**/*.scss' 'packages/*/esnext/**/*.scss'",
    "--------- TESTS:UNIT ---------------": "",
    "test": "node ./tests/nightwatch/commands/loadStoryBook.spec.js && jest --coverage --config jest.json",
    "test:setup": "selenium-standalone install --version=3.4.0 && yarn webpack && yarn build",
    "--------- TESTS:E2E/REGRESSION ----------------": "",
    "regression": "yarn regression:core -- --env chrome_win",
    "regression:xb": "yarn regression:core -- --env firefox_win,chrome_win,ie11,edge,safari_osx",
    "regression:core": "cross-env NODE_ENV=production ./tests/regression/start-test-server.js -o ./tests/regression/tests_output -c ./tests/nightwatch/nightwatch.conf.js --saveDir=new --compareDir=base",
    "regression:review": "node ./tests/regression/start-review-server.js",
    "--------- HELPERS ---------": "",
    "init": "npm i && yarn test:setup && yarn precommit",
    "nuke": "rm -rf node_modules && yarn clean && yarn init",
    "precommit": "yarn lint && npm test",
    "prepush": "yarn e2e",
    "pre-publish": "git fetch && git pull -r && yarn test:setup && yarn lint && yarn test && yarn e2e",
    "preclean": "yarn remove-generated-files && yarn remove-empty-dirs",
    "remove-generated-files": "find . -name package-lock.json -type f -delete && find ./packages -name 'dist' -maxdepth 2 -type d -exec rm -rf {} + ",
    "remove-empty-dirs": "find ./packages -name 'node_modules' -maxdepth 2 -type d -exec rm -rf {} + && find ./packages -type d -empty -delete",
    "preregression:core": "rm -rf ./tests/regression/screenshots/new && rm -rf ./tests/regression/screenshots/errors && rm -rf ./tests/regression/tests_output && rm -rf ./tests/regression/tests_screenshots",
    "publish": "yarn pre-publish && lerna publish",
    "webpack": "cross-env NODE_ENV=production webpack -p"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.1.9",
    "@storybook/addon-knobs": "^3.1.9",
    "@storybook/addon-notes": "3.1.2",
    "@storybook/cli": "^3.1.9",
    "@storybook/react": "^3.1.9",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.13.4",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "progress-bar-webpack-plugin": "^1.9.3",
    "prop-types": "^15.6.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.5",
    "showdown": "^1.7.3",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.1.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "nightwatch": "^0.9.17",
    "browserstack-local": "^1.3.0",
    "chance": "^1.0.9",
    "cross-env": "^5.1.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.0",
    "express": "^4.15.3",
    "html-entities": "^1.2.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "jsdom": "^11.1.0",
    "node-resemble": "^2.0.1",
    "opn": "^5.1.0",
    "png-crop": "0.0.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "selenium-standalone": "^6.5.0",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^1.0.0",
    "stylelint-config-standard": "^17.0.0"
  }
}
